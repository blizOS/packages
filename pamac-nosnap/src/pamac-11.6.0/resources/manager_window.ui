<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <menu id="primary-menu">
    <section>
      <item>
        <attribute name="label" translatable="yes">Software Mode</attribute>
        <attribute name="action">win.software-mode</attribute>
      </item>
      <item>
        <attribute name="label" translatable="yes">Refresh databases</attribute>
        <attribute name="action">win.refresh-databases</attribute>
      </item>
      <item>
        <attribute name="label" translatable="yes">View History</attribute>
        <attribute name="action">win.history</attribute>
      </item>
      <item>
        <attribute name="label" translatable="yes">Install Local Packages</attribute>
        <attribute name="action">win.install-local</attribute>
      </item>
      <item>
        <attribute name="label" translatable="yes">Preferences</attribute>
        <attribute name="action">win.preferences</attribute>
      </item>
      <item>
        <attribute name="label" translatable="yes">About</attribute>
        <attribute name="action">win.about</attribute>
      </item>
    </section>
  </menu>
  <template class="PamacManagerWindow" parent="AdwApplicationWindow">
    <property name="default-width">900</property>
    <property name="default-height">500</property>
    <property name="icon-name">system-software-install</property>
    <signal name="close-request" handler="on_window_close_request" swapped="no"/>
    <child>
      <object class="GtkBox" id="main_box">
        <property name="orientation">vertical</property>
        <child>
          <object class="AdwHeaderBar" id="headerbar">
            <property name="show-end-title-buttons">1</property>
            <child type="title">
              <object class="GtkBox" id="header_center_buttonbox">
                <child>
                  <object class="GtkSearchEntry" id="search_entry">
                    <property name="halign">center</property>
                    <property name="max-width-chars">30</property>
                    <property name="placeholder-text" translatable="yes">Search</property>
                    <signal name="changed" handler="on_search_entry_changed" swapped="no"/>
                    <signal name="search-changed" handler="on_search_entry_search_changed" swapped="no"/>
                    <signal name="activate" handler="on_search_entry_activated" swapped="no"/>
                  </object>
                </child>
                <child>
                  <object class="AdwViewSwitcherTitle" id="view_stack_switcher">
                    <property name="halign">center</property>
                    <property name="stack">view_stack</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="header_left_buttonbox">
                <child>
                  <object class="GtkButton" id="button_back">
                    <property name="action_name">win.back</property>
                    <child>
                      <object class="GtkImage" id="back_image">
                        <property name="icon-name">go-previous-symbolic</property>
                      </object>
                    </child>
                    <style>
                      <class name="image-button"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="search_button">
                    <property name="action_name">win.search</property>
                    <child>
                      <object class="GtkImage" id="search_image">
                        <property name="icon-name">edit-find-symbolic</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child type="end">
              <object class="GtkBox" id="header_right_buttonbox">
                <child>
                  <object class="GtkMenuButton" id="menu_button">
                    <property name="icon-name">open-menu-symbolic</property>
                    <property name="popover">
                      <object class="GtkPopoverMenu" id="menu_popover">
                        <property name="menu_model">primary-menu</property>
                        <signal name="show" handler="on_menu_popover_shown" swapped="no"/>
                      </object>
                    </property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkOverlay" id="overlay1">
            <property name="child">
              <object class="GtkStack" id="main_stack">
                <property name="transition-type">slide-left-right</property>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">browse</property>
                    <property name="title" translatable="yes">Browse</property>
                    <property name="child">
                      <object class="AdwLeaflet" id="packages_leaflet">
                        <property name="mode-transition-duration">200</property>
                        <property name="transition-type">slide</property>
                        <property name="homogeneous">0</property>
                        <property name="can-navigate-back">0</property>
                        <property name="can-navigate-forward">0</property>
                        <child>
                          <object class="AdwLeafletPage">
                            <property name="name">list</property>
                            <property name="child">
                              <object class="AdwFlap" id="browse_flap">
                                <property name="fold-duration">200</property>
                                <property name="fold-policy">never</property>
                                <property name="vexpand">1</property>
                                <child type="content">
                                  <object class="GtkBox" id="browse_box">
                                    <property name="orientation">vertical</property>
                                    <child>
                                      <object class="GtkBox" id="browse_head_box">
                                        <property name="margin-start">6</property>
                                        <property name="margin-end">6</property>
                                        <property name="spacing">18</property>
                                        <child>
                                          <object class="GtkToggleButton" id="reveal_sidebar_button">
                                            <property name="valign">center</property>
                                            <property name="active" bind-source="browse_flap" bind-property="reveal-flap" bind-flags="bidirectional|sync-create"/>
                                            <child>
                                              <object class="GtkImage">
                                                <property name="icon-name">view-sidebar-start-symbolic</property>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkScrolledWindow" id="browse_head_box_scrolledwindow">
                                            <property name="valign">center</property>
                                            <property name="vscrollbar-policy">never</property>
                                            <property name="child">
                                              <object class="GtkBox" id="list_button_box">
                                                <property name="hexpand">1</property>
                                                <property name="halign">end</property>
                                                <property name="margin-top">6</property>
                                                <property name="margin-bottom">6</property>
                                                <property name="spacing">12</property>
                                                <child>
                                                  <object class="GtkButton" id="remove_all_button">
                                                    <property name="label" translatable="yes">Remove all</property>
                                                    <property name="focus-on-click">0</property>
                                                    <property name="receives-default">1</property>
                                                    <property name="valign">center</property>
                                                    <signal name="clicked" handler="on_remove_all_button_clicked" swapped="no"/>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkButton" id="install_all_button">
                                                    <property name="label" translatable="yes">Install all</property>
                                                    <property name="focus-on-click">0</property>
                                                    <property name="valign">center</property>
                                                    <signal name="clicked" handler="on_install_all_button_clicked" swapped="no"/>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkButton" id="ignore_all_button">
                                                    <property name="label" translatable="yes">Ignore all</property>
                                                    <property name="focus-on-click">0</property>
                                                    <property name="receives-default">1</property>
                                                    <property name="valign">center</property>
                                                    <signal name="clicked" handler="on_ignore_all_button_clicked" swapped="no"/>
                                                  </object>
                                                </child>
                                              </object>
                                            </property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkBox" id="sortby_box">
                                            <property name="spacing">6</property>
                                            <property name="margin-top">6</property>
                                            <property name="margin-bottom">6</property>
                                            <child>
                                              <object class="GtkLabel" id="sortby_label">
                                                <property name="ellipsize">end</property>
                                                <property name="xalign">0</property>
                                                <property name="label" translatable="yes">Sort by</property>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkDropDown" id="sortby_dropdown">
                                                <property name="model">
                                                  <object class="GtkStringList">
                                                    <items>
                                                      <item translatable="yes">Relevance</item>
                                                      <item translatable="yes">Name</item>
                                                      <item translatable="yes">Repository</item>
                                                      <item translatable="yes">Size</item>
                                                      <item translatable="yes">Date</item>
                                                    </items>
                                                  </object>
                                                </property>
                                                <property name="valign">center</property>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkToggleButton" id="reveal_details_button">
                                            <property name="visible">0</property>
                                            <property name="valign">center</property>
                                            <property name="active" bind-source="main_details_box" bind-property="visible" bind-flags="bidirectional|sync-create"/>
                                            <child>
                                              <object class="GtkImage">
                                                <property name="icon-name">view-sidebar-end-symbolic</property>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkSeparator" id="browse_separator">
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkStack" id="packages_stack">
                                        <property name="transition-type">crossfade</property>
                                        <child>
                                          <object class="GtkStackPage">
                                            <property name="name">packages</property>
                                            <property name="child">
                                              <object class="GtkScrolledWindow" id="packages_scrolledwindow">
                                                <property name="hscrollbar-policy">never</property>
                                                <property name="child">
                                                  <object class="GtkFlowBox" id="packages_listbox">
                                                    <property name="homogeneous">1</property>
                                                    <property name="row_spacing">6</property>
                                                    <property name="column_spacing">6</property>
                                                    <property name="valign">start</property>
                                                    <property name="halign">center</property>
                                                    <property name="selection_mode">single</property>
                                                    <property name="margin_top">6</property>
                                                    <property name="margin_bottom">6</property>
                                                    <property name="margin_start">6</property>
                                                    <property name="margin_end">6</property>
                                                    <signal name="child-activated" handler="on_packages_listbox_row_activated" swapped="no"/>
                                                  </object>
                                                </property>
                                              </object>
                                            </property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkStackPage">
                                            <property name="name">no_item</property>
                                            <property name="child">
                                              <object class="GtkBox" id="no_item_box">
                                                <property name="orientation">vertical</property>
                                                <property name="spacing">6</property>
                                                <property name="homogeneous">1</property>
                                                <child>
                                                  <object class="GtkImage" id="no_item_icon">
                                                    <property name="valign">end</property>
                                                    <property name="icon-name">action-unavailable-symbolic</property>
                                                    <property name="pixel-size">48</property>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkLabel" id="no_item_label">
                                                    <property name="wrap">1</property>
                                                    <property name="valign">start</property>
                                                  </object>
                                                </child>
                                              </object>
                                            </property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkStackPage">
                                            <property name="name">checking</property>
                                            <property name="child">
                                              <object class="GtkBox" id="checking_box">
                                                <property name="orientation">vertical</property>
                                                <property name="spacing">12</property>
                                                <property name="homogeneous">1</property>
                                                <child>
                                                  <object class="GtkLabel" id="checking_label">
                                                    <property name="wrap">1</property>
                                                  </object>
                                                </child>
                                              </object>
                                            </property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkStackPage">
                                            <property name="name">updated</property>
                                            <property name="child">
                                              <object class="GtkBox" id="updated_box">
                                                <property name="orientation">vertical</property>
                                                <child>
                                                  <object class="GtkBox" id="updated_icon_box">
                                                    <property name="vexpand">1</property>
                                                    <property name="orientation">vertical</property>
                                                    <property name="spacing">6</property>
                                                    <property name="homogeneous">1</property>
                                                    <child>
                                                      <object class="GtkImage" id="updated_icon">
                                                        <property name="valign">end</property>
                                                        <property name="icon-name">emblem-ok-symbolic</property>
                                                        <property name="pixel-size">48</property>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLabel" id="updated_label">
                                                        <property name="wrap">1</property>
                                                        <property name="valign">start</property>
                                                      </object>
                                                    </child>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkBox" id="last_refresh_box">
                                                    <property name="orientation">vertical</property>
                                                    <property name="spacing">12</property>
                                                    <child>
                                                      <object class="GtkButton" id="refresh_button">
                                                        <property name="label" translatable="yes">Refresh</property>
                                                        <property name="receives-default">1</property>
                                                        <property name="halign">center</property>
                                                        <signal name="clicked" handler="on_refresh_button_clicked" swapped="no"/>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLabel" id="last_refresh_label">
                                                        <property name="wrap">1</property>
                                                        <property name="margin-bottom">30</property>
                                                      </object>
                                                    </child>
                                                  </object>
                                                </child>
                                              </object>
                                            </property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </child>
                                <child type="separator">
                                  <object class="GtkSeparator" id="flap_separator">
                                  </object>
                                </child>
                                <child type="flap">
                                  <object class="AdwViewStack" id="view_stack">
                                    <style>
                                      <!--make flap background opaque-->
                                      <class name="background"/>
                                    </style>
                                    <child>
                                      <object class="AdwViewStackPage">
                                        <property name="name">browse</property>
                                        <property name="title" translatable="yes">Browse</property>
                                        <property name="icon_name">globe-symbolic</property>
                                        <property name="child">
                                          <object class="GtkStack" id="browse_stack">
                                            <property name="transition-type">slide-left-right</property>
                                            <child>
                                              <object class="GtkStackPage">
                                                <property name="name">filters</property>
                                                <property name="child">
                                                  <object class="GtkScrolledWindow" id="filters_scrolledwindow">
                                                    <property name="hscrollbar-policy">never</property>
                                                    <property name="child">
                                                      <object class="GtkViewport" id="filters_viewport">
                                                        <property name="child">
                                                          <object class="GtkListBox" id="filters_listbox">
                                                            <property name="selection-mode">none</property>
                                                            <signal name="row-activated" handler="on_filters_listbox_row_activated" swapped="no"/>
                                                            <child>
                                                              <object class="GtkListBoxRow" id="categories_row">
                                                                <property name="selectable">0</property>
                                                                <property name="child">
                                                                  <object class="GtkBox" id="categories_box">
                                                                    <property name="margin-start">12</property>
                                                                    <property name="margin-end">12</property>
                                                                    <property name="margin-top">12</property>
                                                                    <property name="margin-bottom">12</property>
                                                                    <property name="spacing">12</property>
                                                                    <child>
                                                                      <object class="GtkLabel" id="categories_label">
                                                                        <property name="label" translatable="yes">Categories</property>
                                                                        <property name="xalign">0</property>
                                                                        <property name="hexpand">1</property>
                                                                      </object>
                                                                    </child>
                                                                    <child>
                                                                      <object class="GtkImage" id="next_image1">
                                                                        <property name="icon-name">go-next-symbolic</property>
                                                                        <property name="halign">end</property>
                                                                        <property name="valign">center</property>
                                                                      </object>
                                                                    </child>
                                                                  </object>
                                                                </property>
                                                              </object>
                                                            </child>
                                                            <child>
                                                              <object class="GtkListBoxRow" id="groups_row">
                                                                <property name="selectable">0</property>
                                                                <property name="child">
                                                                  <object class="GtkBox" id="groups_box">
                                                                    <property name="margin-start">12</property>
                                                                    <property name="margin-end">12</property>
                                                                    <property name="margin-top">12</property>
                                                                    <property name="margin-bottom">12</property>
                                                                    <property name="spacing">12</property>
                                                                    <child>
                                                                      <object class="GtkLabel" id="groups_label">
                                                                        <property name="label" translatable="yes">Groups</property>
                                                                        <property name="xalign">0</property>
                                                                        <property name="hexpand">1</property>
                                                                      </object>
                                                                    </child>
                                                                    <child>
                                                                      <object class="GtkImage" id="next_image2">
                                                                        <property name="icon-name">go-next-symbolic</property>
                                                                        <property name="halign">end</property>
                                                                        <property name="valign">center</property>
                                                                      </object>
                                                                    </child>
                                                                  </object>
                                                                </property>
                                                              </object>
                                                            </child>
                                                            <child>
                                                              <object class="GtkListBoxRow" id="repos_row">
                                                                <property name="selectable">0</property>
                                                                <property name="child">
                                                                  <object class="GtkBox" id="repos_box">
                                                                    <property name="margin-start">12</property>
                                                                    <property name="margin-end">12</property>
                                                                    <property name="margin-top">12</property>
                                                                    <property name="margin-bottom">12</property>
                                                                    <property name="spacing">12</property>
                                                                    <child>
                                                                      <object class="GtkLabel" id="repos_label">
                                                                        <property name="label" translatable="yes">Repositories</property>
                                                                        <property name="xalign">0</property>
                                                                        <property name="hexpand">1</property>
                                                                      </object>
                                                                    </child>
                                                                    <child>
                                                                      <object class="GtkImage" id="next_image3">
                                                                        <property name="icon-name">go-next-symbolic</property>
                                                                        <property name="halign">end</property>
                                                                        <property name="valign">center</property>
                                                                      </object>
                                                                    </child>
                                                                  </object>
                                                                </property>
                                                              </object>
                                                            </child>
                                                          </object>
                                                        </property>
                                                      </object>
                                                    </property>
                                                  </object>
                                                </property>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkStackPage">
                                                <property name="name">categories</property>
                                                <property name="child">
                                                  <object class="GtkScrolledWindow" id="categories_scolledwindow">
                                                    <property name="child">
                                                      <object class="GtkViewport" id="categories_viewport">
                                                        <property name="child">
                                                          <object class="GtkListBox" id="categories_listbox">
                                                            <property name="selection-mode">browse</property>
                                                            <signal name="row-activated" handler="on_categories_listbox_row_activated" swapped="no"/>
                                                            <style>
                                                              <class name="navigation-sidebar"/>
                                                            </style>
                                                          </object>
                                                        </property>
                                                      </object>
                                                    </property>
                                                  </object>
                                                </property>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkStackPage">
                                                <property name="name">groups</property>
                                                <property name="child">
                                                  <object class="GtkScrolledWindow" id="groups_scrolledwindow">
                                                    <property name="hscrollbar-policy">never</property>
                                                    <property name="child">
                                                      <object class="GtkViewport" id="groups_viewport">
                                                        <property name="child">
                                                          <object class="GtkListBox" id="groups_listbox">
                                                            <property name="selection-mode">browse</property>
                                                            <signal name="row-activated" handler="on_groups_listbox_row_activated" swapped="no"/>
                                                            <style>
                                                              <class name="navigation-sidebar"/>
                                                            </style>
                                                          </object>
                                                        </property>
                                                      </object>
                                                    </property>
                                                  </object>
                                                </property>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkStackPage">
                                                <property name="name">repos</property>
                                                <property name="child">
                                                  <object class="GtkScrolledWindow" id="repos_scrolledwindow">
                                                    <property name="hscrollbar-policy">never</property>
                                                    <property name="vscrollbar-policy">never</property>
                                                    <property name="child">
                                                      <object class="GtkViewport" id="repos_viewport">
                                                        <property name="child">
                                                          <object class="GtkListBox" id="repos_listbox">
                                                            <property name="selection-mode">browse</property>
                                                            <signal name="row-activated" handler="on_repos_listbox_row_activated" swapped="no"/>
                                                            <style>
                                                              <class name="navigation-sidebar"/>
                                                            </style>
                                                          </object>
                                                        </property>
                                                      </object>
                                                    </property>
                                                  </object>
                                                </property>
                                              </object>
                                            </child>
                                          </object>
                                        </property>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="AdwViewStackPage">
                                        <property name="name">installed</property>
                                        <property name="title" translatable="yes">Installed</property>
                                        <property name="icon_name">view-list-symbolic</property>
                                        <property name="child">
                                          <object class="GtkScrolledWindow" id="installed_scolledwindow">
                                            <property name="child">
                                              <object class="GtkViewport" id="installed_viewport">
                                                <property name="child">
                                                  <object class="GtkListBox" id="installed_listbox">
                                                    <property name="selection-mode">browse</property>
                                                    <signal name="row-activated" handler="on_installed_listbox_row_activated" swapped="no"/>
                                                    <style>
                                                      <class name="navigation-sidebar"/>
                                                    </style>
                                                  </object>
                                                </property>
                                              </object>
                                            </property>
                                          </object>
                                        </property>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="AdwViewStackPage">
                                        <property name="name">search</property>
                                        <property name="child">
                                          <object class="GtkScrolledWindow" id="search_scolledwindow">
                                            <property name="child">
                                              <object class="GtkViewport" id="search_viewport">
                                                <property name="child">
                                                  <object class="GtkListBox" id="search_listbox">
                                                    <property name="selection-mode">browse</property>
                                                    <signal name="row-activated" handler="on_search_listbox_row_activated" swapped="no"/>
                                                    <style>
                                                      <class name="navigation-sidebar"/>
                                                    </style>
                                                  </object>
                                                </property>
                                              </object>
                                            </property>
                                          </object>
                                        </property>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="AdwViewStackPage">
                                        <property name="name">updates</property>
                                        <property name="title" translatable="yes">Updates</property>
                                        <property name="icon_name">emblem-synchronizing-symbolic</property>
                                        <property name="child">
                                          <object class="GtkScrolledWindow" id="updates_scolledwindow">
                                            <property name="child">
                                              <object class="GtkViewport" id="updates_viewport">
                                                <property name="child">
                                                  <object class="GtkListBox" id="updates_listbox">
                                                    <property name="selection-mode">browse</property>
                                                    <signal name="row-activated" handler="on_updates_listbox_row_activated" swapped="no"/>
                                                    <style>
                                                      <class name="navigation-sidebar"/>
                                                    </style>
                                                  </object>
                                                </property>
                                              </object>
                                            </property>
                                          </object>
                                        </property>
                                      </object>
                                    </child>
                                  </object>
                                </child>
                              </object>
                            </property>
                          </object>
